{
  "name": "dtw-server",
  "version": "1.0.0",
  "description": "server for Do The Work App ",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "npm run mocha",
    "mocha": "mocha --recursive",
    "test:dev": "nodemon --exec 'npm run mocha'",
    "test:unit": "nodemon --exec 'npm run mocha -- ./test/unit'",
    "test:e2e": "nodemon --exec 'npm run mocha -- ./test/e2e'",
    "start": "node server.js",
    "start:dev": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carriecarter/DTW-Server.git"
  },
  "author": "Carrie Carter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carriecarter/DTW-Server/issues"
  },
  "homepage": "https://github.com/carriecarter/DTW-Server#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jsonwebtoken-promisified": "^1.0.3",
    "mongodb": "^3.1.10",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.10.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  }
}
